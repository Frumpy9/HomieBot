{
  "compilerOptions": {
    "lib": ["ESNext"],
      "target": "ESNext",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "dist",
      "strict": true,
      "importHelpers": true,
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "sourceMap": false,
  },
  "include": ["src", "enviroment.d.ts"],
  "exclude": ["node_modules"]
}